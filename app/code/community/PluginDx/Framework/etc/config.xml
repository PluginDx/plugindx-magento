<config>
    <modules>
        <PluginDx_Framework>
            <version>0.1.0</version>
        </PluginDx_Framework>
    </modules>
    <global>
        <models>
            <plugindx>
                <class>PluginDx_Framework_Model</class>
            </plugindx>
        </models>
        <helpers>
            <plugindx>
                <class>PluginDx_Framework_Helper</class>
            </plugindx>
        </helpers>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <PluginDx_Framework after="Mage_Adminhtml">PluginDx_Framework_Adminhtml</PluginDx_Framework>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
